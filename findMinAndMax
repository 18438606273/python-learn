def findMinAndMax(L):
    if L==[]:
        return (None, None)
    mi=L[0]
    ma=L[0]
    for n in L:
        if n>ma or n<mi:
            if n>ma:
                ma=n
            else:
                mi=n
        
    return (mi,ma)
if findMinAndMax([]) != (None, None):
    print('测试失败!')
elif findMinAndMax([7]) != (7, 7):
    print('测试失败!')
elif findMinAndMax([7, 1]) != (1, 7):
    print('测试失败!')
elif findMinAndMax([7, 1, 3, 9, 5]) != (1, 9):
    print('测试失败!')
else:
    print('测试成功!')
